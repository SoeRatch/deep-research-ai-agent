# API Keys for LLM Providers (Choose at least 2)
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Google Gemini
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Search API
TAVILY_API_KEY=your_tavily_api_key_here

# Model Configuration
PRIMARY_MODEL=openai  # Options: openai, google, anthropic
SECONDARY_MODEL=anthropic  # Options: openai, google, anthropic
PRIMARY_MODEL_NAME=gpt-4-turbo-preview  # or gemini-2.0-flash-exp, claude-opus-4
SECONDARY_MODEL_NAME=claude-opus-4  # or gpt-4-turbo-preview, gemini-2.0-flash-exp

# Agent Configuration
MAX_RESEARCH_DEPTH=5  # Maximum consecutive search iterations
MAX_QUERIES_PER_ITERATION=3  # Max queries per search round
CONFIDENCE_THRESHOLD=0.7  # Stop if confidence reaches this level

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=20
REQUEST_TIMEOUT_SECONDS=30

# Output Configuration
OUTPUT_DIR=reports
AUDIT_LOGS_ENABLED=true
